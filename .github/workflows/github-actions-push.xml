name: GitHub Actions On Push
on: [push]
jobs:
  Build-App:
    runs-on: windows-latest
    steps:
    - uses: actions/checkout@v3
    - name: Setup dotnet
      uses: actions/setup-dotnet@v2
      with:
        dotnet-version: dotnet-version: '6.0.x'          
    - run: dotnet build WingetUI.csproj
